{
    "manifest_version": 2,
    "name": "refresher",
    "version": "1.0",
    "description": "Auto refresh page",
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon/lemonade.png",
        "default_title": "Auto Refresh"
    }
}
